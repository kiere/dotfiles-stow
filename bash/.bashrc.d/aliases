alias reload='source ~/.bashrc'
alias aliases='nvim ~/.bashrc.d/aliases'
alias ll='ls -AlGh'

# Git aliases
alias gst='git status'
alias gc='git commit'
alias gca='git commit -a'
alias gd='git diff'
alias gdi='git diff --cached'

# Project aliases
alias dotfiles='cd ~/dotfiles'
alias outdoor='cd ~/work/code/outdoor'
alias ginger='cd ~/work/code/ginger'
